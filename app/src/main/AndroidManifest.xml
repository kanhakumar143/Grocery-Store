<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kanhakumar.modokan">

    <application
        android:allowBackup="true"
        android:icon="@drawable/image_store"
        android:label="@string/app_name"
        android:roundIcon="@drawable/image_store"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activitys.PlacedOrderActivity"></activity>
        <activity android:name=".activitys.NavCategoryActivity" />
        <activity android:name=".activitys.DetailedActivity" />
        <activity android:name=".activitys.ViewAllActivity" />
        <activity android:name=".activitys.Welcome">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activitys.RegistrationActivity" />
        <activity android:name=".activitys.LoginActivity" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>